beginSubroutine: s32(FBall_Shot_Init) {
	specialAttack: 
	callSubroutine: s32(cmnAtkLevel_3_AtkInit)
	callSubroutine: s32(cmnSpecialShot_AtkInit)
	damage: 900
	atkHoseiTadan: 1
	hitstop: 10
	groundKnockbackEffect: (AIR_FACE_UP)
	attackXKnockback: 20000
	attackYKnockback: 25000
	airHitstun: 24
	autoHitSignalSending: 1
	atkIgnoreCaution: 1
	atkFrontDirection: 0
	atkShakeCamera: s32(ShakeTate), 600, 0, 25, 10
	worldOutToEliminate: 1
	landingHeight: 0
	setActionTimeMax: 600
	xSpeedPerFrame: 102
	ySpeedPerFrame: 102
	setSpeedByCurrentAngle: 80000, 0
	ifOperation: (IS_EQUAL), var(act5), int(10) {
		setAngle: 18000
		attackYKnockback: 35000
		setSpeedByCurrentAngle: 80000, 0
		attackWhiffCrouch: 1
		damageScalingOverride: 1000
		enemyHitstopAddition: 0, 1, 1
		isAirUnblockable: 1
	} endIf: 
	ifOperation: (IS_EQUAL), var(act5), int(20) {
		callSubroutine: s32(cmnSpecialAttackEx_ShotInit)
		setSpeedByCurrentAngle: 100000, 0
		attackXKnockback: 20000
		attackYKnockback: 42000
		airHitstun: 45
		enemyHitstopAddition: 0, 5, 5
		attackGravity: 1500
		guardPushback: 50
	} endIf: 
	ifOperation: (IS_EQUAL), var(act5), int(21) {
		callSubroutine: s32(cmnSpecialAttackEx_ShotInit)
		setSpeedByCurrentAngle: 100000, 0
		groundKnockbackEffect: (SPIN)
		airKnockbackEffect: (SPIN)
		attackXKnockback: 20000
		attackYKnockback: 42000
		airHitstun: 45
		enemyHitstopAddition: 0, 5, 5
		attackGravity: 1500
		guardPushback: 50
	} endIf: 
	checkCurrentStateName: (PLAYER), s32(AssistAttack)
	if: var(Result) {
		copyVar: var(act5), int(30)
		setSpeedByCurrentAngle: 70000, 0
		damage: 800
		attackXKnockback: 15000
		attackYKnockback: 28000
		airHitstun: 30
		hitstop: 15
		callSubroutine: s32(cmn_AssistHosei)
	} endIf: 
	pushXSpeed: 
	pushYSpeed: 
	upon: (AFTER_EXIT) {
		clearRegisteredUponCode: (AFTER_EXIT)
		copyVar: var(act6), int(1)
	} endUpon: 
	upon: (DESTROY) {
		deactivateObject: (SELF)
		createObject: s32(cmn_kidan_end_M), (ZERO)
		ifNot: var(act6) {
			checkCurrentStateName: (PLAYER), s32(FBall_A)
			if: var(Result) {
				sendSignal: (PLAYER), (SIGNAL0)
			} endIf: 
			checkCurrentStateName: (PLAYER), s32(FBall_B)
			if: var(Result) {
				sendSignal: (PLAYER), (SIGNAL0)
			} endIf: 
			checkCurrentStateName: (PLAYER), s32(FBall_C)
			if: var(Result) {
				checkCurrentStateName: (SELF), s32(FBall_C_Shot2)
				if: var(Result) {
					sendSignal: (PLAYER), (SIGNAL0)
				} endIf: 
			} endIf: 
		} endIf: 
	} endUpon: 
	upon: (ATTACK_COLLISION_MAIN_PLAYER) {
		clearRegisteredUponCode: (ATTACK_COLLISION_MAIN_PLAYER)
		requestDestroy: (SELF)
	} endUpon: 
	upon: (ATTACK_HIT_PLAYER_ARMOR) {
		clearRegisteredUponCode: (ATTACK_HIT_PLAYER_ARMOR)
		requestDestroy: (SELF)
	} endUpon: 
	upon: (ATTACK_HIT_MAIN_PLAYER_DAMAGE) {
		clearRegisteredUponCode: (ATTACK_HIT_MAIN_PLAYER_DAMAGE)
		damage: 900
		ifOperation: (IS_EQUAL), var(act5), int(30) {
			damage: 800
		} endIf: 
	} endUpon: 
	upon: (ATTACK_COLLISION_SOUSAI) {
		copyVar: var(act1), int(0)
		upon: (FRAME_STEP_AFTER_EX) {
			ifNot: var(CheckStop) {
				modifyVar: (ADD), var(act1), int(1)
				ifOperation: (IS_GREATER_OR_EQUAL), var(act1), int(5) {
					clearRegisteredUponCode: (FRAME_STEP_AFTER_EX)
					resetSpeed: 1
					popXSpeed: 
					popYSpeed: 
				} endIf: 
			} endIf: 
		} endUpon: 
		enableHitbox: 
		damage: 200
		setXSpeed: 30000
		addXPosition: -110000
		ifOperation: (IS_EQUAL), var(act5), int(10) {
			setYSpeed: 10000
			addYPosition: -30000
		} endIf: 
		modifyVar: (ADD), var(act0), int(1)
		ifOperation: (IS_GREATER_OR_EQUAL), var(act0), int(4) {
			requestDestroy: (SELF)
		} endIf: 
		else:  {
			ifNot: var(act6) {
				checkCurrentStateName: (PLAYER), s32(FBall_A)
				if: var(Result) {
					sendSignal: (PLAYER), (SIGNAL2)
				} endIf: 
				checkCurrentStateName: (PLAYER), s32(FBall_B)
				if: var(Result) {
					sendSignal: (PLAYER), (SIGNAL2)
				} endIf: 
			} endIf: 
		} endElse: 
	} endUpon: 
	upon: (IDLING) {
		ifOperation: (IS_LESS_OR_EQUAL), var(PosY), int(400000) {
			effectForLandBreak: 2, 1000, 0, 0
		} endIf: 
	} endUpon: 
	upon: (TOUCH_GROUND) {
		deactivateObject: (SELF)
		effectForLandBreak: 0, 1000, 0, 0
		createParticle: s32(bg_groundsmokeM), (ZERO)
	} endUpon: 
	damageSoundEffect: s32(ARC_BTL_CMN_Hit_kidan)
	guardSoundEffect: s32(ARC_BTL_CMN_Guard_Kidan)
	characterSfx: s32(ARC_BTL_KFS_FBall_Shot)
} endSubroutine: 

